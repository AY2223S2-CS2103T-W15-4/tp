@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":InternEaseParser" as InternEaseParser LOGIC_COLOR
participant ":ClearByCommandParser" as ClearByCommandParser LOGIC_COLOR
participant "c:ClearByCommand" as ClearByCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "model:Model" as Model MODEL_COLOR
participant "internEase:InternEase" as InternEase MODEL_COLOR
participant ":UniqueApplicationList" as UniqueApplicationList MODEL_COLOR
end box
[-> LogicManager : execute(clear_by)
activate LogicManager

LogicManager -> InternEaseParser : parseCommand(clear_by)
activate InternEaseParser

create ClearByCommandParser
InternEaseParser -> ClearByCommandParser : parse(clear_by)
activate ClearByCommandParser

ClearByCommandParser --> InternEaseParser
deactivate ClearByCommandParser

InternEaseParser -> ClearByCommandParser : parse(argument)
activate ClearByCommandParser

create ClearByCommand
ClearByCommandParser -> ClearByCommand
activate ClearByCommand

ClearByCommand --> ClearByCommandParser
deactivate ClearByCommand

ClearByCommandParser --> InternEaseParser
deactivate ClearByCommandParser
ClearByCommandParser -[hidden]-> InternEaseParser : result
destroy ClearByCommandParser

InternEaseParser --> LogicManager : c
deactivate InternEaseParser

LogicManager -> ClearByCommand : execute()
activate ClearByCommand

ClearByCommand -> Model : getFilteredInternshipList()
activate Model

Model -> InternEase : getInternshipList()
activate InternEase

InternEase -> UniqueApplicationList :asUnmodifiableObservableList()
activate UniqueApplicationList

UniqueApplicationList --> InternEase
deactivate UniqueApplicationList

InternEase --> Model
deactivate InternEase

Model --> ClearByCommand : internshipList
deactivate Model

ClearByCommand -> ClearByCommand : getFilteredList(internshipList) : filteredList

loop size of filteredList

ClearByCommand -> Model : addInternshipToCache(application)
activate Model

Model --> ClearByCommand
deactivate Model

ClearByCommand -> Model : deleteInternship(application)
activate Model

Model --> ClearByCommand
deactivate Model

end

create CommandResult
ClearByCommand -> CommandResult
activate CommandResult

CommandResult --> ClearByCommand
deactivate CommandResult

ClearByCommand --> LogicManager : r
deactivate ClearByCommand


[<--LogicManager
deactivate LogicManager
@enduml
